{
	"expo": {
		"name": "luma",
		"slug": "luma",
		"version": "0.1.0",
		"orientation": "portrait",
		"icon": "./assets/images/luma-icon.png",
		"scheme": "luma",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"githubUrl": "https://github.com/Yuhanawa/luma",
		"platforms": ["ios", "android"],
		"ios": {
			"supportsTablet": true,
			"infoPlist": {
				"LSApplicationQueriesSchemes": ["https", "http"]
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/luma-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "io.github.yuhanawa.luma"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/luma-icon@3x.png",
					"imageWidth": 516,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "8d57e4d3-fca7-413c-931c-d48b6eaafbee"
			}
		}
	}
}
